@using System.Collections.ObjectModel
@using DynamicData.Binding
<MudTable Elevation="0" T="string" Items="@Messages" Dense="true" Height="400px">
                <RowTemplate>
                    <MudTd>@context</MudTd>
                </RowTemplate>
            </MudTable>
@code {
    [Parameter]
    public ObservableCollection<string> Messages { get; set; }

    protected override void OnInitialized()
    {
        Messages.ToObservableChangeSet().Subscribe(set => InvokeAsync(StateHasChanged));

    }

}